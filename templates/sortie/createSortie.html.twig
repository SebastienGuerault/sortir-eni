{% extends 'base.html.twig' %}

{% block title %}Créer une sortie{% endblock %}

{% block body %}

    <h1>Créer la sortie</h1>

    {{ form_start(registrationForm) }}


    <div class="form_alignement">

        {{ form_row(registrationForm.nom) }}
        {{ form_row(registrationForm.dateHeureDebut) }}
        {{ form_row(registrationForm.duree) }}
        {{ form_row(registrationForm.dateLimiteInscription) }}
        {{ form_row(registrationForm.nbInscriptionMax) }}
        {{ form_row(registrationForm.infoSortie) }}
        {{ form_row(registrationForm.lieu) }}
    </div>


    <br>

        <button type="submit" class="btn">Créer la sortie</button>
    {{ form_end(registrationForm) }}
{% endblock %}
